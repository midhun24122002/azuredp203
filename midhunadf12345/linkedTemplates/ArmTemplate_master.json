{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "midhunadf12345"
		},
		"ADLSg2_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSg2_LS'"
		},
		"AdventueworksSQLDB_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdventueworksSQLDB_LS'"
		},
		"ADLSg2_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://midhundatalakedemo.dfs.core.windows.net/"
		},
		"AdventueworksSQLDB_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "midhuntrainingsql.database.windows.net"
		},
		"AdventueworksSQLDB_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Adventureworks"
		},
		"AdventueworksSQLDB_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "midhun"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "midhunadf12345_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSg2_LS_accountKey": {
						"value": "[parameters('ADLSg2_LS_accountKey')]"
					},
					"AdventueworksSQLDB_LS_password": {
						"value": "[parameters('AdventueworksSQLDB_LS_password')]"
					},
					"ADLSg2_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLSg2_LS_properties_typeProperties_url')]"
					},
					"AdventueworksSQLDB_LS_properties_typeProperties_server": {
						"value": "[parameters('AdventueworksSQLDB_LS_properties_typeProperties_server')]"
					},
					"AdventueworksSQLDB_LS_properties_typeProperties_database": {
						"value": "[parameters('AdventueworksSQLDB_LS_properties_typeProperties_database')]"
					},
					"AdventueworksSQLDB_LS_properties_typeProperties_userName": {
						"value": "[parameters('AdventueworksSQLDB_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}